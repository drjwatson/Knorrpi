{
    "functions": {
      "api/server.js": {
        "memory": 1024,
        "includeFiles": "knorrpi.json"
      }
    },
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/api/server.js"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/server.js"
      },
      {
        "src": "/",
        "dest": "/index.html"
      }
    ]
}